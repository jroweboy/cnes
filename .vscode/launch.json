{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": ["--famistudio-path=C:/dev/cnes/tools/famistudio/FamiStudio.exe", "C:/dev/cnes/example/assets/audio", "C:/dev/cnes/example/build/pc/gen"]
    },
    {
      "name": "g++.exe - Build and debug cmake",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/example/build/pc/cnes_example.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C/C++: g++.exe cmake build"
    }
  ]
}